  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <style>

  body {
    shape-rendering: crispEdges;
  }

  .day {
    fill: #fff;
    stroke: #ccc;
  }

  .month {
    fill: none;
    stroke: #000;
    stroke-width: 2px;
  }

  .colours .q0-5{fill:rgb(230,230,230)}
  .colours .q1-5{fill:rgb(214,232,129)}
  .colours .q2-5{fill:rgb(142,200,96)}
  .colours .q3-5{fill:rgb(74,165,57)}
  .colours .q4-5{fill:rgb(35,105,29)}
  .colours .q5-5{fill:rgb(255,255,191)}

  </style>
  <body>
</head>

<body>
  <h1><%= link_to "Logs", root_path %></h1>

<%= yield %>

</body>
